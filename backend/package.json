{
  "name": "music-market",
  "version": "0.0.1",
  "description": "",
  "author": "Khalo Mykyta <khalo.nikita@gmail.com>",
  "private": true,
  "license": "MIT",
  "engines": {
    "npm": ">=10.0.0 <11.0.0",
    "node": ">=18.0.0 <21.0.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"src/database/**\"",
    "prettier": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:prod:logs": "NEW_RELIC_NO_CONFIG_FILE=true NEW_RELIC_APP_NAME=$NEW_RELIC_APP_NAME NEW_RELIC_LICENSE_KEY=$NEW_RELIC_LICENSE_KEY node -r newrelic dist/main.js",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\"  --fix",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest-e2e.json",
    "test:unit": "jest --config jest-unit.json",
    "db:migrate": "ts-node node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo": "ts-node node_modules/.bin/sequelize db:migrate:undo",
    "comment": "next line might break the ci/cd pipeline",
    "prepare": "cd .. && husky install backend/.husky"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.0",
    "@adminjs/nestjs": "^6.1.0",
    "@adminjs/sequelize": "^4.0.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.438.0",
    "@aws-sdk/client-s3": "^3.386.0",
    "@aws-sdk/client-ses": "^3.387.0",
    "@aws-sdk/s3-request-presigner": "^3.405.0",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.1.3",
    "@nestjs/swagger": "^7.1.2",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/websockets": "^10.2.8",
    "@socket.io/redis-adapter": "^8.2.1",
    "@types/multer": "^1.4.11",
    "adminjs": "^7.4.2",
    "aws-sdk": "^2.1423.0",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "bluebird": "^3.7.2",
    "cache-manager": "^5.2.4",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.11.0",
    "handlebars": "^4.7.7",
    "moment": "^2.29.4",
    "nest-winston": "^1.9.4",
    "newrelic": "^11.10.3",
    "newrelic-winston": "^2.1.0",
    "node-lame": "^1.3.2",
    "nodemailer": "^6.9.4",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.5",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "sql-formatter": "^15.1.2",
    "stripe": "^13.2.0",
    "winston": "^3.11.0",
    "winston-cloudwatch": "^6.2.0",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/newrelic": "^9.14.3",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "sequelize-cli": "^6.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "cd backend && npm run lint:fix",
      "cd backend && npm run prettier"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
